{
  "SYNTAX": "MOSDEXSchemaV1-3-ajk.json",
  "sailcoSolve": {
    "CLASS": "ALGORITHM",
    "NAME": "sailcoSolve",
    "INITIALIZE": [
      "sailco"
    ],
    "HEADING": {
      "DESCRIPTION": [
        "Just initialization at this point"
      ]
    }
  },
  "sailco": {
    "CLASS": "MODULE",
    "HEADING": {
      "DESCRIPTION": [
        "Inventory problem with lagged decision variables"
      ],
      "VERSION": [
        "1-2"
      ],
      "REFERENCE": [
        "https://www.ibm.com/support/knowledgecenter/SSSA5P_12.7.1/ilog.odms.ide.help/OPL_Studio/opllanguser/topics/opl_languser_app_areas_pwl_inventory.html"
      ],
      "AUTHOR": [
        "Jeremy A. Bloom (jeremyblmca@gmail.com)"
      ],
      "NOTICES": [
        "Copyright Â© 2019 Jeremy A. Bloom"
      ],
      "MATH": [
        "minimize",
        "sum( t in Periods ) ( RegularCost * Regular[t] ) +",
        "sum( t in Periods ) ( ExtraCost * Extra[t] ) +",
        "sum( t in Periods ) ( InventoryCost * Inventory[t] );",
        "subject to {",
        "forall( t in Periods )",
        "ctCapacity[t]: Regular[t] <= Capacity;",
        "forall( t in Periods )",
        "ctBoat[t]:",
        "if (t>1)",
        "Regular[t] + Extra[t] + Inventory[t-1] - Inventory[t] == Demand[t];",
        "else //t=1",
        "Regular[t] + Extra[t] - Inventory[t] == Demand[t] - InitialInventory;",
        "}"
      ]
    },
    "periods": {
      "CLASS": "DATA",
      "TYPE": "INPUT",
      "METADATA": {
        "item_name": "Periods",
        "item_type": "INTEGER",
        "item_usage": "KEY"
      },
      "SCHEMA": {
        "period": "INTEGER"
      },
      "INSTANCE": [
        [
          1
        ],
        [
          2
        ],
        [
          3
        ],
        [
          4
        ]
      ],
      "NAME": "periods"
    },
    "demands": {
      "CLASS": "DATA",
      "TYPE": "INPUT",
      "SCHEMA": {
        "period": "INTEGER",
        "demand": "DOUBLE"
      },
      "METADATA": {
        "item_name": [
          "Periods",
          "Demands"
        ],
        "item_type": [
          "INTEGER",
          "DOUBLE"
        ],
        "item_usage": [
          "FOREIGN_KEY",
          "VALUE"
        ]
      },
      "INSTANCE": [
        [
          1,
          40.0
        ],
        [
          2,
          60.0
        ],
        [
          3,
          72.0
        ],
        [
          4,
          25.0
        ]
      ],
      "NAME": ""
    },
    "parameters": {
      "NAME": "parameters",
      "CLASS": "DATA",
      "TYPE": "INPUT",
      "SCHEMA": {
        "name": "STRING",
        "value": "DOUBLE"
      },
      "METADATA": {
        "item_name": [
          "Name",
          "Value"
        ],
        "item_type": [
          "STRING",
          "DOUBLE"
        ],
        "item_usage": [
          "KEY",
          "VALUE"
        ]
      },
      "INSTANCE": [
        [
          "regularCost",
          400.0
        ],
        [
          "extraCost",
          450.0
        ],
        [
          "capacity",
          40.0
        ],
        [
          "initialInventory",
          10.0
        ],
        [
          "inventoryCost",
          20.0
        ]
      ]
    },
    "production": {
      "CLASS": "DATA",
      "NAME": "production",
      "TYPE": "OUTPUT",
      "METADATA": {
        "item_name": [
          "Periods",
          "Regular",
          "Extra",
          "Inventory",
          "MarginalCapacityValue"
        ],
        "item_type": [
          "INTEGER",
          "DOUBLE",
          "DOUBLE",
          "DOUBLE",
          "DOUBLE"
        ],
        "item_usage": [
          "FOREIGN_KEY",
          "VARIABLE_VALUE",
          "VARIABLE_VALUE",
          "VARIABLE_VALUE",
          "DUAL"
        ]
      },
      "RECIPE": [
        {
          "DIRECTIVE": "SELECT",
          "PREDICATE": [
            "sailco_periods.period ",
            "regular.Value AS Regular",
            "extra.Value AS Extra",
            "inventory.Value AS Inventory",
            "ctCapacity.Dual AS MarginalCapacityValue"
          ]
        },
        {
          "DIRECTIVE": "FROM",
          "PREDICATE": [
            "sailco_periods.periods"
          ]
        },
        {
          "DIRECTIVE": "JOIN",
          "PREDICATE": [
            "regular",
            "USING",
            "period"
          ]
        },
        {
          "DIRECTIVE": "JOIN",
          "PREDICATE": [
            "extra",
            "USING",
            "period"
          ]
        },
        {
          "DIRECTIVE": "JOIN",
          "PREDICATE": [
            "inventory",
            "USING",
            "period"
          ]
        },
        {
          "DIRECTIVE": "JOIN",
          "PREDICATE": [
            "ctCapacity",
            "USING",
            "period"
          ]
        }
      ]
    },
    "regular": {
      "CLASS": "VARIABLE",
      "TYPE": "CONTINUOUS",
      "METADATA": {
        "item_name": [
          "Periods",
          "Column",
          "LowerBound",
          "UpperBound",
          "Objective",
          "Value"
        ],
        "item_type": [
          "INTEGER",
          "STRING",
          "DOUBLE",
          "DOUBLE",
          "DOUBLE",
          "DOUBLE"
        ],
        "item_usage": [
          "FOREIGN_KEY",
          "KEY",
          "LOWER_BOUND",
          "UPPER_BOUND",
          "OBJECTIVE_COEFFICIENT",
          "VARIABLE_VALUE"
        ]
      },
      "RECIPE": [
        {
          "DIRECTIVE": "SELECT",
          "PREDICATE": [
            "period ",
            "'regular' ||'_' || period AS Column",
            "0.0 AS LowerBound",
            "'infinity' AS UpperBound",
            "sailco_parameters.value AS Objective",
            "0.0 AS Value"
          ]
        },
        {
          "DIRECTIVE": "FROM",
          "PREDICATE": [
            "sailco_periods",
            "sailco_parameters"
          ]
        },
        {
          "DIRECTIVE": "WHERE",
          "PREDICATE": [
            "sailco_parameters.name == 'regularCost'"
          ]
        }
      ],
      "NAME": "regular"
    },
    "extra": {
      "CLASS": "VARIABLE",
      "TYPE": "CONTINUOUS",
      "METADATA": {
        "item_name": [
          "Periods",
          "Column",
          "LowerBound",
          "UpperBound",
          "Objective",
          "Value"
        ],
        "item_type": [
          "INTEGER",
          "STRING",
          "DOUBLE",
          "DOUBLE",
          "DOUBLE",
          "DOUBLE"
        ],
        "item_usage": [
          "FOREIGN_KEY",
          "KEY",
          "LOWER_BOUND",
          "UPPER_BOUND",
          "OBJECTIVE_COEFFICIENT",
          "VARIABLE_VALUE"
        ]
      },
      "RECIPE": [
        {
          "DIRECTIVE": "SELECT",
          "PREDICATE": [
            "period",
            "'extra' || '_' || period AS Column",
            "0.0 AS LowerBound",
            "'infinity' AS UpperBound",
            "sailco_parameters.value AS Objective",
            "0.0 AS Value"
          ]
        },
        {
          "DIRECTIVE": "FROM",
          "PREDICATE": [
            "sailco_periods",
            "sailco_parameters"
          ]
        },
        {
          "DIRECTIVE": "WHERE",
          "PREDICATE": [
            "sailco_parameters.name == 'extraCost'"
          ]
        }
      ],
      "NAME": "regular"
    },
    "inventory": {
      "CLASS": "VARIABLE",
      "TYPE": "CONTINUOUS",
      "METADATA": {
        "item_name": [
          "Periods",
          "Column",
          "Inventory",
          "UpperBound",
          "Objective",
          "Value"
        ],
        "item_type": [
          "INTEGER",
          "STRING",
          "DOUBLE",
          "DOUBLE",
          "DOUBLE",
          "DOUBLE"
        ],
        "item_usage": [
          "FOREIGN_KEY",
          "KEY",
          "LOWER_BOUND",
          "UPPER_BOUND",
          "OBJECTIVE_COEFFICIENT",
          "VARIABLE_VALUE"
        ]
      },
      "RECIPE": [
        {
          "DIRECTIVE": "SELECT",
          "PREDICATE": [
            "period",
            "'inventory' || '_' || period AS Column",
            "0.0 AS LowerBound",
            "'infinity' AS UpperBound",
            "sailco_parameters.value AS Objective",
            "0.0 AS Value"
          ]
        },
        {
          "DIRECTIVE": "FROM",
          "PREDICATE": [
            "sailco_periods",
            "sailco_parameters"
          ]
        },
        {
          "DIRECTIVE": "WHERE",
          "PREDICATE": [
            "sailco_parameters.name == 'extraCost'"
          ]
        }
      ],
      "NAME": "inventory"
    },
    "ctCapacity": {
      "CLASS": "CONSTRAINT",
      "TYPE": "LINEAR",
      "METADATA": {
        "item_name": [
          "Periods",
          "Row",
          "Sense",
          "RHS",
          "Value"
        ],
        "item_type": [
          "INTEGER",
          "STRING",
          "STRING",
          "DOUBLE",
          "DOUBLE"
        ],
        "item_usage": [
          "FOREIGN_KEY",
          "KEY",
          "SENSE",
          "RHS",
          "CONSTRAINT_VALUE"
        ]
      },
      "RECIPE": [
        {
          "DIRECTIVE": "SELECT",
          "PREDICATE": [
            "period ",
            "'ctCapacity' || '_' ||  period AS Row",
            "'LE' AS Sense",
            "sailco_parameters.value AS RHS"
          ]
        },
        {
          "DIRECTIVE": "FROM",
          "PREDICATE": [
            "sailco_periods",
            "sailco_parameters"
          ]
        },
        {
          "DIRECTIVE": "WHERE",
          "PREDICATE": [
            "sailco_parameters.name == 'capacity'"
          ]
        }
      ],
      "NAME": "ctCapacity"
    },
    "ctBoat": {
      "CLASS": "CONSTRAINT",
      "TYPE": "LINEAR",
      "METADATA": {
        "item_name": [
          "Periods",
          "Row",
          "Sense",
          "RHS",
          "Value"
        ],
        "item_type": [
          "INTEGER",
          "STRING",
          "STRING",
          "DOUBLE",
          "DOUBLE"
        ],
        "item_usage": [
          "FOREIGN_KEY",
          "KEY",
          "SENSE",
          "RHS",
          "CONSTRAINT_VALUE"
        ]
      },
      "RECIPE": [
        {
          "DIRECTIVE": "SELECT",
          "PREDICATE": [
            "period AS Period",
            "'ctBoat' || '_' ||  period AS Row",
            "'EQ' AS Sense",
            "demand AS RHS"
          ]
        },
        {
          "DIRECTIVE": "FROM",
          "PREDICATE": [
            "sailco_demands"
          ]
        },
        {
          "DIRECTIVE": "WHERE",
          "PREDICATE": [
            "period > 1"
          ]
        }
      ],
      "NAME": "ctBoat"
    },
    "ctBoat_1": {
      "CLASS": "CONSTRAINT",
      "TYPE": "LINEAR",
      "METADATA": {
        "item_name": [
          "Periods",
          "Row",
          "Sense",
          "RHS",
          "Value"
        ],
        "item_type": [
          "INTEGER",
          "STRING",
          "STRING",
          "DOUBLE",
          "DOUBLE"
        ],
        "item_usage": [
          "FOREIGN_KEY",
          "KEY",
          "SENSE",
          "RHS",
          "CONSTRAINT_VALUE"
        ]
      },
      "RECIPE": [
        {
          "DIRECTIVE": "SELECT",
          "PREDICATE": [
            "sailco_demands.period ",
            "'ctBoat_1' AS Row",
            "'EQ' AS Sense",
            "(sailco_demands.demand - sailco_parameters.value) AS RHS"
          ]
        },
        {
          "DIRECTIVE": "FROM",
          "PREDICATE": [
            "sailco_parameters",
            "sailco_demands"
          ]
        },
        {
          "DIRECTIVE": "WHERE",
          "PREDICATE": [
            "sailco_demands.period == 1"
          ]
        },
        {
          "DIRECTIVE": "AND",
          "PREDICATE": [
            "sailco_parameters.name == 'initialInventory'"
          ]
        }
      ],
      "NAME": "ctBoat_1"
    },
    "totalCost": {
      "CLASS": "OBJECTIVE",
      "TYPE": "LINEAR",
      "METADATA": {
        "item_name": [
          "Value"
        ],
        "item_type": [
          "DOUBLE"
        ],
        "item_usage": [
          "OBJECTIVE_VALUE"
        ]
      },
      "RECIPE": [
        {
          "DIRECTIVE": "SELECT",
          "PREDICATE": [
            "SUM( sailco_regular.objective * sailco_regular.value + sailco_extra.objective * sailco_extra.value + sailco_inventory.objective * sailco_inventory.value )"
          ]
        },
        {
          "DIRECTIVE": "FROM",
          "PREDICATE": [
            "sailco_regular",
            "sailco_extra",
            "sailco_inventory"
          ]
        }
      ],
      "NAME": "totalCost"
    },
    "ctCapacity_regular": {
      "CLASS": "TERM",
      "TYPE": "LINEAR",
      "METADATA": {
        "item_name": [
          "Row",
          "Column",
          "Value"
        ],
        "item_type": [
          "STRING",
          "STRING",
          "DOUBLE"
        ],
        "item_usage": [
          "FOREIGN_KEY",
          "FOREIGN_KEY",
          "MATRIX_ENTRY"
        ]
      },
      "RECIPE": [
        {
          "DIRECTIVE": "SELECT",
          "PREDICATE": [
            "'ctCapacity' || '_' ||  period AS Row",
            "'regular' || '_' ||  period AS Column",
            "1.0 AS Coefficient"
          ]
        },
        {
          "DIRECTIVE": "FROM",
          "PREDICATE": [
            "sailco_periods"
          ]
        }
      ],
      "NAME": "ctCapacity_regular"
    },
    "ctBoat_regular": {
      "CLASS": "TERM",
      "TYPE": "LINEAR",
      "METADATA": {
        "item_name": [
          "Row",
          "Column",
          "Value"
        ],
        "item_type": [
          "STRING",
          "STRING",
          "DOUBLE"
        ],
        "item_usage": [
          "FOREIGN_KEY",
          "FOREIGN_KEY",
          "MATRIX_ENTRY"
        ]
      },
      "RECIPE": [
        {
          "DIRECTIVE": "SELECT",
          "PREDICATE": [
            "'ctBoat' || '_' ||  period AS Row",
            "'regular' || '_' ||  period AS Column",
            "1.0 AS Coefficient"
          ]
        },
        {
          "DIRECTIVE": "FROM",
          "PREDICATE": [
            "sailco_periods"
          ]
        }
      ],
      "NAME": "ctBoat_regular"
    },
    "ctBoat_extra": {
      "CLASS": "TERM",
      "TYPE": "LINEAR",
      "METADATA": {
        "item_name": [
          "Row",
          "Column",
          "Value"
        ],
        "item_type": [
          "STRING",
          "STRING",
          "DOUBLE"
        ],
        "item_usage": [
          "FOREIGN_KEY",
          "FOREIGN_KEY",
          "MATRIX_ENTRY"
        ]
      },
      "RECIPE": [
        {
          "DIRECTIVE": "SELECT",
          "PREDICATE": [
            "'ctBoat' || '_' ||  period AS Row",
            "'extra' || '_' ||  period AS Column",
            "1.0 AS Coefficient"
          ]
        },
        {
          "DIRECTIVE": "FROM",
          "PREDICATE": [
            "sailco_periods"
          ]
        }
      ],
      "NAME": "ctBoat_extra"
    },
    "ctBoat_inventory": {
      "CLASS": "TERM",
      "TYPE": "LINEAR",
      "METADATA": {
        "item_name": [
          "Row",
          "Column",
          "Value"
        ],
        "item_type": [
          "STRING",
          "STRING",
          "DOUBLE"
        ],
        "item_usage": [
          "FOREIGN_KEY",
          "FOREIGN_KEY",
          "MATRIX_ENTRY"
        ]
      },
      "RECIPE": [
        {
          "DIRECTIVE": "SELECT",
          "PREDICATE": [
            "'ctBoat' || '_' ||  period AS Row",
            "'inventory' || '_' ||  period AS Column",
            "-1.0 AS Coefficient"
          ]
        },
        {
          "DIRECTIVE": "FROM",
          "PREDICATE": [
            "sailco_periods"
          ]
        }
      ],
      "NAME": "ctBoat_inventory"
    },
    "ctBoat_lagged_inventory": {
      "CLASS": "TERM",
      "TYPE": "LINEAR",
      "METADATA": {
        "item_name": [
          "Row",
          "Column",
          "Value"
        ],
        "item_type": [
          "STRING",
          "STRING",
          "DOUBLE"
        ],
        "item_usage": [
          "FOREIGN_KEY",
          "FOREIGN_KEY",
          "MATRIX_ENTRY"
        ]
      },
      "RECIPE": [
        {
          "DIRECTIVE": "SELECT",
          "PREDICATE": [
            "'ctBoat' || '_' ||  period AS Row",
            "'inventory' || '_' ||  (period - 1) AS Column",
            "1.0 AS Coefficient"
          ]
        },
        {
          "DIRECTIVE": "FROM",
          "PREDICATE": [
            "sailco_periods"
          ]
        },
        {
          "DIRECTIVE": "WHERE",
          "PREDICATE": [
            "sailco_periods.period > 1"
          ]
        }
      ],
      "NAME": "ctBoat_lagged_inventory"
    },
    "NAME": "ctBoat_lagged_inventory"
  }
}